<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@+id/mainFragment">

    <fragment
        android:id="@+id/animatorFragment"
        android:name="happy.mjstudio.animationsample.ui.AnimatorFragment"
        android:label="fragment_animator"
        tools:layout="@layout/fragment_animator"/>

    <fragment
        android:id="@+id/mainFragment"
        android:name="happy.mjstudio.animationsample.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">

        <action
            android:id="@+id/action_mainFragment_to_animatorFragment"
            app:destination="@id/animatorFragment"
            app:enterAnim="@anim/item_animation_fall_down"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_animatedVectorDrawableFragment"
            app:destination="@id/animatedVectorDrawableFragment" />
        <action
            android:id="@+id/action_mainFragment_to_animateLayoutChangesFragment"
            app:destination="@id/animateLayoutChangesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_layoutAnimationFragment"
            app:destination="@id/layoutAnimationFragment" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_circleRevealFragment"
            app:destination="@id/circleRevealFragment" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_sceneFragment"
            app:destination="@id/sceneFragment" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_constraintSetFragment"
            app:destination="@id/constraintSetFragment" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_lottieFragment"
            app:destination="@id/lottieFragment" />
        <action
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            android:id="@+id/action_mainFragment_to_XMLAnimFragment"
            app:destination="@id/XMLAnimFragment" />
        <action
            android:id="@+id/action_mainFragment_to_materialMotionFragment"
            app:destination="@id/materialMotionFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>


    <fragment
        android:id="@+id/animatedVectorDrawableFragment"
        android:name="happy.mjstudio.animationsample.ui.AnimatedVectorDrawableFragment"
        android:label="fragment_animated_vector_drawable"
        tools:layout="@layout/fragment_animated_vector_drawable" />
    <fragment
        android:id="@+id/animateLayoutChangesFragment"
        android:name="happy.mjstudio.animationsample.ui.LayoutTransitionFragment"
        android:label="fragment_animate_layout_changes"
        tools:layout="@layout/fragment_layout_transition" />
    <fragment
        android:id="@+id/circleRevealFragment"
        android:name="happy.mjstudio.animationsample.ui.CircleRevealFragment"
        android:label="fragment_circle_reveal"
        tools:layout="@layout/fragment_circle_reveal" />
    <fragment
        android:id="@+id/layoutAnimationFragment"
        android:name="happy.mjstudio.animationsample.ui.LayoutAnimationFragment"
        android:label="fragment_layout_animation"
        tools:layout="@layout/fragment_layout_animation" />
    <fragment
        android:id="@+id/sceneFragment"
        android:name="happy.mjstudio.animationsample.ui.SceneFragment"
        android:label="SceneFragment"
        tools:layout="@layout/fragment_scene" />
    <fragment
        android:id="@+id/constraintSetFragment"
        android:name="happy.mjstudio.animationsample.ui.ConstraintSetFragment"
        android:label="ConstraintSetFragment"
        tools:layout="@layout/fragment_constraint_set" />
    <fragment
        tools:layout="@layout/fragment_lottie"
        android:id="@+id/lottieFragment"
        android:name="happy.mjstudio.animationsample.ui.LottieFragment"
        android:label="LottieFragment" />
    <fragment
        tools:layout="@layout/fragment_xml_anim"
        android:id="@+id/XMLAnimFragment"
        android:name="happy.mjstudio.animationsample.ui.XMLAnimFragment"
        android:label="XMLAnimFragment" />
    <fragment
        android:id="@+id/materialMotionFragment"
        android:name="happy.mjstudio.animationsample.ui.MaterialMotionFragment"
        android:label="MaterialMotionFragment"
        tools:layout="@layout/fragment_material_motions">
        <action
            android:id="@+id/action_materialMotionFragment_to_materialContainerTransformFragment"
            app:destination="@id/materialContainerTransformFragment" />
        <action
            android:id="@+id/action_materialMotionFragment_to_materialElevationScaleFragment"
            app:destination="@id/materialElevationScaleFragment" />
        <action
            android:id="@+id/action_materialMotionFragment_to_materialFadeThroughFragment"
            app:destination="@id/materialFadeThroughFragment" />
        <action
            android:id="@+id/action_materialMotionFragment_to_materialSharedAxisFragment"
            app:destination="@id/materialSharedAxisFragment" />
    </fragment>

    <fragment
        android:id="@+id/materialElevationScaleFragment"
        android:name="happy.mjstudio.animationsample.ui.MaterialElevationScaleFragment"
        android:label="MaterialElevationScaleFragment"
        tools:layout="@layout/fragment_material_elevation_scale" />
    <fragment
        android:id="@+id/materialSharedAxisFragment"
        android:name="happy.mjstudio.animationsample.ui.MaterialSharedAxisFragment"
        android:label="MaterialSharedAxisFragment"
        tools:layout="@layout/fragment_material_shared_axis" />
    <fragment
        android:id="@+id/materialFadeThroughFragment"
        android:name="happy.mjstudio.animationsample.ui.MaterialFadeThroughFragment"
        android:label="MaterialFadeThroughFragment"
        tools:layout="@layout/fragment_material_fade_through" />
    <fragment
        android:label="MaterialContainerTransformFragment"
        android:id="@+id/materialContainerTransformFragment"
        android:name="happy.mjstudio.animationsample.ui.MaterialContainerTransformFragment"
        tools:layout="@layout/fragment_material_container_transform">
        <argument
            android:name="itemText"
            app:argType="string" />
    </fragment>


</navigation>